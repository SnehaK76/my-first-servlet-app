<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>StudentServlet2</servlet-name>
		<servlet-class>com.prowings.servlet.StudentServlet2</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StudentServlet2</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>FetchStudentDetailsStudent</servlet-name>
		<servlet-class>com.prowings.servlet.FetchStudentDetailsStudent</servlet-class>
		
	</servlet>
	<servlet-mapping>
		<servlet-name>FetchStudentDetailsStudent</servlet-name>
		<url-pattern>/getStudent</url-pattern>
	</servlet-mapping>

 </web-app>